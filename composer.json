{
	"name" : "foundee/api",
	"description" : "API written in Symfony for Foundee forum application",
	"keywords" : [
		"foundee",
		"forum",
		"api"
	],
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": ">=7.2",
		"ext-iconv": "*",
		"lexik/jwt-authentication-bundle": "^2.6",
		"nelmio/api-doc-bundle": "^3.1",
		"symfony/asset": "^4.0",
		"symfony/console": "^4.0",
		"symfony/flex": "^1.0",
		"symfony/form": "^4.0",
		"symfony/framework-bundle": "^4.0",
		"symfony/lts": "^4@dev",
		"symfony/mailer": "^4.0",
		"symfony/orm-pack": "^1.0",
		"symfony/security": "^4.0",
		"symfony/security-bundle": "^4.0",
		"symfony/serializer-pack": "^1.0",
		"symfony/translation": "^4.0",
		"symfony/twig-bundle": "^4.0",
		"symfony/validator": "^4.0",
		"symfony/yaml": "^4.0"
	},
	"require-dev" : {
		"symfony/browser-kit": "^4.0",
		"symfony/dotenv": "^4.0",
		"symfony/maker-bundle": "^1.14",
		"symfony/phpunit-bridge": "^4.0",
		"symfony/profiler-pack": "^1.0"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"id" : "01C4WVB25RAA92HPJJ57RJP9SR",
			"allow-contrib" : false
		}
	}
}
